{
  "extends": "next/core-web-vitals",
  "plugins": ["prettier"],
  "rules": {
      "prettier/prettier": [
          "error",
          { "endOfLine": "auto" }
      ],
      "arrow-body-style": ["warn", "always"],
      "curly": ["warn", "all"],
      "react/self-closing-comp": ["warn", { "component": true, "html": true }]
  }
}
